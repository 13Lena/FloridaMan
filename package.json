    "bcrypt-nodejs": "0.0.3",
    "bootstrap": "^4.3.1",
    "connect-flash": "^0.1.1",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.5",
    "morgan": "^1.9.1",
    "nightmare": "^3.0.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-session": "^1.0.2",
